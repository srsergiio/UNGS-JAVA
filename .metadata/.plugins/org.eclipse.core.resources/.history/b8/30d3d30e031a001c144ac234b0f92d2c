package P1;



public class Practiva3 {

	
double ArrMin(double[]arr){
	double Min;
for (int i =0;i<arr.length;i++) {
	if(arr[1]<Min) {
		Min=arr[i];
	}else {
		Min=Min[i-1];
	}
}
	return Min;
}
